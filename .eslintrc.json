{
  "extends": ["next", "next/core-web-vitals", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prefer-const": "error",
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "if"
      }
    ],
    "no-console": "error",
    "quotes": "error",
    "no-unused-vars": "error"
  }
}
